{
	"name": "df_stores_silver",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_stores_bronze",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_stores_silver",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          store_id as string,",
				"          store_name as string,",
				"          city as string,",
				"          country as string,",
				"          opened_year as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 derive(store_name = iif(store_id == 'ST-9', replace(split(store_name, ',')[1], \"'\", \"\"), store_name),",
				"          city = iif(store_id == 'ST-9', split(store_name, ',')[2], city),",
				"          country = iif(store_id == 'ST-9', split(store_name, ',')[3], country),",
				"          opened_year = iif(store_id == 'ST-9', split(store_name, ',')[4], opened_year)) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(store_id),",
				"     each(match(name!=\"store_id\"), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          store_id as string,",
				"          store_name as string,",
				"          city as string,",
				"          country as string,",
				"          opened_year as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}